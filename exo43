#include "my_header.h"
#include "stdlib.h"
#include <math.h>

#define	RAND_MAX	32767

int main(void)

{
    unsigned long long S1=1;
    unsigned long n=3;

    unsigned int S3=0;
    int v=0;
    int a= (rand()%10);

    int S4=0;
    int c= 0;
    int d=1;

    while(S1<9876)
    {
        S1=S1*n;
        n=n+3;
    }

    while (v<12)
    {
        S3= S3+a;
        v=v+1;
        a=(rand()%10);
    }

    while(c<20)
    {
        if (d%3==0 || c%7==0)
        {
            S4=S4+d;
            d=d+1;
            c=c+1;
        }
         else
            c=c+1;
    }
    return 0;
}
